{
  "name": "next-prisma-starter",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=12.*"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "yarn db:migrate",
    "start": "next start -p $PORT",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy --preview-feature && yarn db:generate",
    "db:gen:migration": "prisma migrate dev --preview-feature",
    "db:reset": "prisma migrate reset --preview-feature",
    "lint": "eslint src --ext ts,tsx,js,jsx"
  },
  "dependencies": {
    "@prisma/client": "2.13.1",
    "bcrypt": "^5.0.0",
    "cookie": "^0.4.1",
    "jsonwebtoken": "^8.5.1",
    "next": "10.1.3",
    "next-rpc": "https://github.com/jfranciscosousa/next-rpc#fs/app-js-fix",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-error-boundary": "^3.1.0",
    "react-hook-form": "^6.13.1",
    "redaxios": "^0.4.1",
    "superjson": "^1.4.1",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "@prisma/cli": "2.13.1",
    "@tailwindcss/forms": "^0.2.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.16",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "autoprefixer": "^10.1.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-finiam": "^1.0.2",
    "eslint-config-import": "^0.13.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "postcss": "^8.2.1",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  }
}
