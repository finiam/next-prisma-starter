generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model Note {
  id        String        @id @default(uuid())
  content   String
  createdAt DateTime      @default(now())
  updatedAt DateTime      @updatedAt
  userId    String
  user      User          @relation(fields: [userId], references: [id])
}

model User {
  id        String        @id @default(uuid())
  email     String        @unique
  name      String
  password  String
  createdAt DateTime      @default(now())
  updatedAt DateTime      @updatedAt
  Note      Note[]
}
